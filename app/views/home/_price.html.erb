        </div>
        <div class="col-2 mt-4 mr-5">
          <iframe id="getOurApp" scrolling="no" allowtransparency="true" src="https://clients.mindbodyonline.com/connect/appbutton?siteID=251698" style="border: none; width: 200px; height: 48px;"></iframe>
        </div>
        <div class="col-7 mt-4 ml-2 text-right">
          <%= image_tag "prices/payment_methods.png", width: "170" %>
        </div>
      </div>
    </div>
  </div>
</section>
